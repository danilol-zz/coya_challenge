<!DOCTYPE html>
<html>
  <head>
    <title>CoyaChallenge</title>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag 'favicon48x48.ico' %>
  </head>

  <body>
    <div class="berlin-background">
      <div class="uk-container uk-flex uk-flex-center uk-flex-middle uk-height-viewport">
        <div class="uk-width-1-2 uk-width-1-2@m uk-flex-middle" uk-grid>
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
